  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: Flask",
      "type": "python",
      "request": "launch",
      "module": "flask",
      "python": "${workspaceFolder}/backend/python3-virtualenv/Scripts/python.exe",
      "env": {
        "FLASK_APP": "${workspaceFolder}/backend/app.py",
        "FLASK_ENV": "development"
      },
      "args": ["run", "--no-debugger"],
      "jinja": true
    }
  ]
}
